<div class="container p-4">
    <div class="row">
        <div class="col-md-4">
           <div class="card">
               <div class="card-bady">
                   <h1>Mis lista</h1>
                     
               </div>
           </div> 
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <table class="table table-dark">
                    <h1>Usuarios</h1>
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">identificador</th>
                            <th scope="col">ususario</th>
                            <th scope="col">nombre</th>
                            <th scope="col">edad</th>
                            <th scope="col">identificacion</th>
                            <th scope="col">telefono</th>
                             <th scope="col">*</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        {{#each usua}}
                        <tr>
                            <th scope="row">{{id}}</th>
                            <td>{{nombre}}</td>
                            <td>{{nombres}}</td>
                            <td>{{pass}}</td>
                            <td>{{edad}}</td>
                            <td>{{identificacion}}</td>
                            <td>{{telefono}}</td>
                            <td><a href="" class="btn btn-info">modificar</a></td>
                            <td><a href="delete/{{id}}" class="btn btn-danger">eliminar</a></td>
                        </tr>
                         {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>